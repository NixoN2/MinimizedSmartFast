#!/home/smartcontract/SmartContract/smartfast/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'smartfast-analyzer==0.0.4','console_scripts','smartfast-format'
__requires__ = 'smartfast-analyzer==0.0.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('smartfast-analyzer==0.0.4', 'console_scripts', 'smartfast-format')()
    )
